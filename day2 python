# if condition
a=int(input("enter the age"))
if a>=18:
  print("eligble for vote")

# odd and even number 
a=int(input("enter the number"))
if a%3==0:
  print("even number")
else:
  print("odd number")

# if elif condition
a=int(input("enter the marks"))
if a>=95:
  print("excellent")
elif a>=75:
   print("good")
elif a==50:
  print("just pass")
else :
    print("fail")

# if elif condition
a=float(input("enter the grade"))
if a>=90:
  print("Grade A")
elif a>=70:
  print("Grade B")
elif a>=50:
  print("Grade C")
else:
  print("fail")

#character type checker
alpha=str(input("enter the alphabet"))
if alpha in "aeiou":
  print("the alphabet is a vowel")
else:
   print("the alphabet is a consonant")

#divisibility test
num=float(input("enter the number"))
if num%3==0:
  print(num,"is divisible by 3")
elif num%5==0:
  print(num,"is divisible by 5")
else:
  print(num,"is not divisible by both 3 and 5")

# if elif condition
a=int(input("enter the number"))
if a>90:
   print("the number is positive")
elif a<60:
   print("the number is negative")
else:
     print("the is zero")

# largest two number
num1=float(input("enter the first number"))
num2=float(input("enter the second number"))
if num1>num2:
   print(num1 ,"the largest number is ")
elif num1<num2:
    print(num2,"the largest number is")
else:
       print("both the number are equal")     

#largest three number
num1=int(input("enter the first number"))
num2=int(input("enter the second number"))
num3=int(input("enter the thrid number"))
if num1<num2:
   print(num2 ,"the largest number is")
elif num2<num3:
    print(num3, "the largest number is")
else:
     print(num1,"the largest number is")

# leap year
year=int(input("enter the year"))
if year%4==0:
 print(year,"is a leap year")
else:
    print(year,"is a leap year")

#voting eligibility
a=int(input("enter the age"))
if a>=18:
  print("eligibilty for voting")
else:
     print("not eligibilty for voting")     

for sinchana in range(1,26):
  if sinchana%4==0:
   print("even") 
  else:
   print("odd")


